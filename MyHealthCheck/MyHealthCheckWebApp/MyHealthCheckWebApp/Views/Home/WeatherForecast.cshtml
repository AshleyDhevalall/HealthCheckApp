@{
  ViewData["Title"] = "Weather Forecast";
}
<h1 class="display-4">@ViewData["Title"]</h1>
<br/>

@{
  var weatherforecasts = ViewBag.WeatherForecast;

  <table id="customers">
    <thead>
      <tr>
        <th>Date</th>
        <th>Temp. (C)</th>
        <th>Temp. (F)</th>
        <th>Summary</th>
      </tr>
    </thead>
    <tbody>
      @foreach (var item in (List<MyHealthCheckWebApp.Models.WeatherForecast>)ViewBag.WeatherForecasts)
      {
      <tr>
        <td>@item.date</td>
        <td>@item.temperatureC</td>
        <td>@item.temperatureF</td>
        <td>@item.summary</td>
      </tr>
      }
      </tbody>
  </table>
}